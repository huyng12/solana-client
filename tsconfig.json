{
	"compilerOptions": {
		"target": "es2020",
		"module": "commonjs",
		"lib": ["es2020"],
		"rootDir": "./src",
		"moduleResolution": "node",
		"outDir": "./dist",
		"declaration": true,
		"declarationDir": "./dist/types",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
